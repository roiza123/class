.container {
  display: flex;
  height: 100vh;
  background-color: rgb(176, 220, 213);
  flex-direction: column;
  text-align: center;
  align-items: center;
}

.logo {
  height: 30vh;
  background-image: linear-gradient(rgb(104, 152, 134)4vh, rgb(226, 235, 179)15vh, rgb(176, 220, 213)22vh);
  width: 30vh;
  font-size: 30px;
  display: flex;
  font-family: YouYuan;
  font-weight: bolder;
  border-radius: 50%;
  justify-content: center;
  margin: 20% auto 0;
}

text {
  height: 15vh;
  width: 15vh;
  color: white;
  text-align: center;
  margin-top: 6vh;
}

form {
  display: flex;
  flex-direction: column;
  text-align: center;
  align-items: center;
  width: 94%;
}

input {
  font-size: 16px;
  font-weight: normal;
  border: white 1px solid;
  background-color: white;
  border-radius: 10px;
}

button {
  margin-top: 100rpx;
  background-color: rgb(226, 235, 179);
  color: rgb(104, 152, 134);
  height: 100rpx;
  width: 150rpx;
}

.box {
  display: flex;
  flex-direction: row;
  color: rgb(104, 152, 134);
  font-size: 18px;
  font-family: 微软雅黑;
  font-weight: bold;
  margin: 40rpx auto;
  text-align: center;
  align-items: center;
}

radio-group {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.box>image{
  height: 24px;
  width: 24px;
  margin-left: 8px;
}