.container {
  display: flex;
  height: 100vh;
  background-color: #b0dcd5;
  display: flex;
  flex-direction: column;
  text-align: center;
  align-items: center;
  width: 100%;
}

.user-box {
  display: flex;
  flex-direction: row;
  width: 94%;
  background-color: white;
  border-radius: 10px;
  margin-bottom: 8px;
  height: 200rpx;
  text-align: center;
  justify-content: space-between;
  align-items: center;
}

.user-box .box {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.user-box .box>image {
  width: 60px;
  height: 60px;
  border-radius: 50%;
  margin-left: 30rpx;
}

.user-box .box>text{
  margin-left: 10px;
  font-size: 18px;
}

.user-box>text {
  margin-right: 30rpx;
  padding: 20rpx;
  background-color: rgb(104, 152, 134);
  border-radius: 30px;
  font-size: 10px;
}

.menu{
  display: flex;
  flex-direction: row;
  align-items: center;
  text-align: center;
  justify-content: space-evenly;
  width: 100%;
  background-color: white;
  border-radius: 10px;
  width: 94%;
  height: 80px;
  margin-bottom: 8px;
}

.menu-detail{
  display: flex;
  flex-direction: column;
  text-align: center;
  align-items: center;
}

.menu-detail>.img{
  height: 70rpx;
  width: 70rpx;
  background-color:rgb(104, 152, 134) ;
  padding: 15rpx;
  border-radius: 50%;
  margin-bottom: 10rpx;
}

.menu-detail>text{
  font-size: 12px;
}

.nothinglist{
  background-color: white;
  height: 268px;
  border-radius: 10px;
  width: 94%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.nothing{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background-color:rgb(226, 235, 179);
  height: 40px;
  line-height: 40px;
  margin:0 6px;
  border-radius: 8px;
}

.nothing>text{
  margin:0 8px;
}